<Window x:Class="CalculoEngenharia.VIEW.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculoEngenharia.VIEW"
		xmlns:app="clr-namespace:CalculoEngenharia"
        mc:Ignorable="d"
		MinHeight="{Binding Path=minHeight, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
		MinWidth="{Binding Path=minWidth, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
		Height="400"
		Width="400"
        Title="{Binding Path=titulo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Icon="..\CEIcon.ico"
		>
	
	<ContentPresenter Margin="10" Content="{Binding Path=conteudo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
</Window>

